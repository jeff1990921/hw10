      "source": [
        "import random\n",
        "ans = random.randint(1,100)\n",
        "while True:\n",
        "  guess = int(input('guess:'))\n",
        "  if guess > ans:\n",
        "    print('too big')\n",
        "  elif guess < ans:\n",
        "    print('too small')\n",
        "  else:\n",
        "    print('hit')\n",
        "    break"
      ],
